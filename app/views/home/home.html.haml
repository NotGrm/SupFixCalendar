.row
	.twothird.centered
		- if flash[:error]
			%p.error= flash[:error]
		- if flash[:notice]
			%p.notice= flash[:notice]

		= form_tag "/home/file_upload", :multipart => true , :class => "forms" do 
			%fieldset
				%ul
					%li.custominput
						%label.bold{"for" => "ics"} Your .ics
						= file_field_tag "ics",  :required => true
					%li
						= submit_tag 'Go !', :class => "btn"
 
